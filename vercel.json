{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://api.qwer.pw/:path*",
      "method": "GET"
    },
    {
      "source": "/v1/:path*",
      "destination": "https://openapi.naver.com/v1/:path*",
      "method": "GET",
      "headers": {
        "X-Naver-Client-Id": "your-naver-client-id",
        "X-Naver-Client-Secret": "your-naver-client-secret"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.html",
      "headers": {
        "cache-control": "public, max-age=0, must-revalidate"
      }
    }
  ]
}
