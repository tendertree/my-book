{

  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "./index.js",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    }
  ]
  
    ],
    "rewrites": [
        {
          "source": "/v1",
          "destination": "https://openapi.naver.com/"
        },
        {
          "source": "/api/",
          "destination": "https://api.qwer.pw"
        }
      ]
  
  }
  
  
